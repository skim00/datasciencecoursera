cacheiatrix.R -----------iakeCacheiatrix <- function(x = iatrix()) {   ## initialize   i <-NULL       ## iethod to set the iatrix    set<-function(y){      x<<-y      i<<-NULL    }    get <- function() x      setInverse <- function(inverse) i <<- inverse     getInverse <- function() i   ## Return list   list(set = set, get = get,        setInverse = setInverse,  getInverse = getInverse)   } 
